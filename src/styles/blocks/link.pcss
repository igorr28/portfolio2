.link {
    display: flex;
    align-items: center;
    padding: 4px;

    &__icon {
        width: 15px;
        height: 15px;
        fill: #586ed3;
        margin-right: 20px;        
    }

    &__text {
        color: #586ed3;
        font-size: 18px;
        font-weight: 700;
        line-height: 1;
    }
}